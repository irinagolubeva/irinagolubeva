{
  "source": "./contents",
  "destination": "./build",
  "metadata": {
    "title": "Irina Golubeva personal website"
  },
  "plugins": {
    "metalsmith-collections": {
      "articles": {
        "sortBy": "date",
        "reverse": true
      }
    },
    "metalsmith-markdown": true,
    "metalsmith-layouts": {
      "engine": "jade",
      "directory": "./templates"
    },
    "metalsmith-in-place": {
      "engine": "jade"
    },
    "./plugins/published": true,
    "metalsmith-assets": {
      "source": "./public",
      "destination": "."
    }
  }
}